{
  "name": "tensorfleet-drone",
  "displayName": "TensorFleet Drone Suite",
  "description": "Flexible drone operations workspace for TensorFleet with multi-view dashboards and tooling management.",
  "version": "0.0.1",
  "publisher": "tensorfleet",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:tensorfleet-qgroundcontrol",
    "onView:tensorfleet-gazebo",
    "onView:tensorfleet-aiops",
    "onView:tensorfleet-ros2-baselines",
    "onView:tensorfleet-vm-manager",
    "onView:tensorfleet-tooling-view",
    "onCommand:tensorfleet.openQGroundControlPanel",
    "onCommand:tensorfleet.openGazeboPanel",
    "onCommand:tensorfleet.openAIPanel",
    "onCommand:tensorfleet.openROS2Panel",
    "onCommand:tensorfleet.createNewProject",
    "onCommand:tensorfleet.installTools",
    "onCommand:tensorfleet.openAllPanels",
    "onCommand:tensorfleet.startMCPServer",
    "onCommand:tensorfleet.stopMCPServer",
    "onCommand:tensorfleet.getMCPConfig",
    "onCommand:tensorfleet.selectRosVersion",
    "onCommand:tensorfleet.showDroneStatus",
    "onCommand:tensorfleet.openVMManagerPanel",
    "onCommand:tensorfleet.startVMManager",
    "onCommand:tensorfleet.stopVMManager",
    "onCommand:tensorfleet.showVMManagerLogs",
    "onCommand:tensorfleet.selectVMManagerEnvironment",
    "onCommand:tensorfleet.vmManager.login",
    "onCommand:tensorfleet.vmManager.logout"
  ],
  "main": "./out/extension.js",
  "packageManager": "bun@1.0.0",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tensorfleet-drone-suite",
          "title": "TensorFleet",
          "icon": "media/tensorfleet-icon.svg"
        }
      ],
      "panel": [
        {
          "id": "tensorfleet-tooling",
          "title": "TensorFleet Tools",
          "icon": "media/tensorfleet-icon.svg"
        }
      ]
    },
    "views": {
      "tensorfleet-drone-suite": [
        {
          "id": "tensorfleet-qgroundcontrol",
          "name": "QGroundControl",
          "type": "webview"
        },
        {
          "id": "tensorfleet-gazebo",
          "name": "Gazebo World",
          "type": "webview"
        },
        {
          "id": "tensorfleet-aiops",
          "name": "AI Ops",
          "type": "webview"
        },
        {
          "id": "tensorfleet-ros2-baselines",
          "name": "ROS 2 & Stable Baselines",
          "type": "webview"
        },
        {
          "id": "tensorfleet-vm-manager",
          "name": "VM Manager",
          "type": "webview"
        }
      ],
      "tensorfleet-tooling": [
        {
          "id": "tensorfleet-tooling-view",
          "name": "Tool Installer",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "tensorfleet.openQGroundControlPanel",
        "title": "TensorFleet: Open QGroundControl Workspace",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.openGazeboPanel",
        "title": "TensorFleet: Open Gazebo Workspace",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.openAIPanel",
        "title": "TensorFleet: Open AI Ops Workspace",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.openROS2Panel",
        "title": "TensorFleet: Open ROS 2 & Stable Baselines Lab",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.createNewProject",
        "title": "TensorFleet: Create New Project",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.installTools",
        "title": "TensorFleet: Install Bundled Tools",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.openAllPanels",
        "title": "TensorFleet: Open All Dashboards in Main Area",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.startMCPServer",
        "title": "TensorFleet: Start MCP Server (for Cursor/Claude)",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.stopMCPServer",
        "title": "TensorFleet: Stop MCP Server",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.getMCPConfig",
        "title": "TensorFleet: Show MCP Configuration",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.selectRosVersion",
        "title": "TensorFleet: Select ROS Version",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.showDroneStatus",
        "title": "TensorFleet: Show Drone Status",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.openVMManagerPanel",
        "title": "TensorFleet: Open VM Manager Console",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.startVMManager",
        "title": "TensorFleet: Start VM Manager Service",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.stopVMManager",
        "title": "TensorFleet: Stop VM Manager Service",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.showVMManagerLogs",
        "title": "TensorFleet: Show VM Manager Logs",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.selectVMManagerEnvironment",
        "title": "TensorFleet: Select VM Manager Environment",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.vmManager.login",
        "title": "TensorFleet: Log In to VM Manager",
        "category": "TensorFleet"
      },
      {
        "command": "tensorfleet.vmManager.logout",
        "title": "TensorFleet: Log Out of VM Manager",
        "category": "TensorFleet"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "tensorfleet.openQGroundControlPanel",
          "when": "view == tensorfleet-qgroundcontrol",
          "group": "navigation@1"
        },
        {
          "command": "tensorfleet.openGazeboPanel",
          "when": "view == tensorfleet-gazebo",
          "group": "navigation@1"
        },
        {
          "command": "tensorfleet.openAIPanel",
          "when": "view == tensorfleet-aiops",
          "group": "navigation@1"
        },
        {
          "command": "tensorfleet.openROS2Panel",
          "when": "view == tensorfleet-ros2-baselines",
          "group": "navigation@1"
        },
        {
          "command": "tensorfleet.openAllPanels",
          "when": "view == tensorfleet-qgroundcontrol",
          "group": "navigation@9"
        },
        {
          "command": "tensorfleet.openAllPanels",
          "when": "view == tensorfleet-gazebo",
          "group": "navigation@9"
        },
        {
          "command": "tensorfleet.openAllPanels",
          "when": "view == tensorfleet-aiops",
          "group": "navigation@9"
        },
        {
          "command": "tensorfleet.openAllPanels",
          "when": "view == tensorfleet-ros2-baselines",
          "group": "navigation@9"
        },
        {
          "command": "tensorfleet.openAllPanels",
          "when": "view == tensorfleet-tooling-view",
          "group": "navigation@9"
        },
        {
          "command": "tensorfleet.openAllPanels",
          "when": "view == tensorfleet-vm-manager",
          "group": "navigation@9"
        },
        {
          "command": "tensorfleet.selectVMManagerEnvironment",
          "when": "view == tensorfleet-vm-manager",
          "group": "navigation@2"
        },
        {
          "command": "tensorfleet.vmManager.login",
          "when": "view == tensorfleet-vm-manager",
          "group": "navigation@3"
        },
        {
          "command": "tensorfleet.vmManager.logout",
          "when": "view == tensorfleet-vm-manager",
          "group": "navigation@4"
        }
      ]
    },
    "configuration": {
      "title": "TensorFleet",
      "properties": {
        "tensorfleet.vmManager.repoPath": {
          "type": "string",
          "default": "../vm-manager",
          "description": "Path to the Go vm-manager repository. Relative paths are resolved from the extension or current workspace."
        },
        "tensorfleet.vmManager.apiBaseUrl": {
          "type": "string",
          "default": "http://localhost:8080",
          "description": "Base URL for the vm-manager HTTP API used by the VM console webview."
        },
        "tensorfleet.vmManager.environments": {
          "type": "array",
          "default": [
            {
              "id": "local",
              "label": "Local Dev",
              "apiBaseUrl": "http://localhost:8080",
              "authEndpoint": "http://localhost:8080/login"
            },
            {
              "id": "staging",
              "label": "Staging",
              "apiBaseUrl": "https://staging.vm.tensorfleet.dev",
              "authEndpoint": "https://staging.vm.tensorfleet.dev/login"
            },
            {
              "id": "production",
              "label": "Production",
              "apiBaseUrl": "https://vm.tensorfleet.com",
              "authEndpoint": "https://vm.tensorfleet.com/login"
            }
          ],
          "description": "List of available VM Manager environments. Each entry should include an id, friendly label, API base URL, and optional auth endpoint.",
          "items": {
            "type": "object",
            "required": [
              "id",
              "label",
              "apiBaseUrl"
            ],
            "properties": {
              "id": {
                "type": "string",
                "description": "Unique identifier for the environment (e.g. local, staging, production)."
              },
              "label": {
                "type": "string",
                "description": "Human-readable label shown in pickers."
              },
              "apiBaseUrl": {
                "type": "string",
                "description": "Base URL of the VM Manager API."
              },
              "authEndpoint": {
                "type": "string",
                "description": "Optional absolute or relative URL to use for login requests. Defaults to /login."
              },
              "defaultEmail": {
                "type": "string",
                "description": "Optional hint email that will be pre-filled in the login form."
              }
            }
          }
        },
        "tensorfleet.vmManager.activeEnvironment": {
          "type": "string",
          "default": "local",
          "description": "Identifier of the active VM Manager environment. Must match one of the configured environments."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "bun run compile",
    "compile": "bunx tsc -p ./",
    "watch": "bunx tsc -watch -p ./",
    "lint": "bunx eslint src --ext ts",
    "package": "bunx vsce package",
    "mcp": "node out/mcp-server.js"
  },
  "bin": {
    "tensorfleet-mcp": "./out/mcp-server.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.17.0",
    "@types/vscode": "^1.85.0",
    "eslint": "^8.56.0",
    "typescript": "^5.2.2",
    "tslib": "^2.6.2"
  }
}
